
compile: ufc

SRC_DIR = $(TOP_DIR)/src
include $(TOP_DIR)/Make.config

ufc: .obj/.phony $(DEFS) $(FIC_OBJS) .obj/extra/hart_io.o $(TOP_DIR)/utils/universal_file_converter/universal-file-converter.c
	$(CC) $(CFLAGS) -I. -I$(SRC_DIR)/base -I$(SRC_DIR)/core $(INCLUDES) $(FIC_OBJS) .obj/extra/hart_io.o $(TOP_DIR)/utils/universal_file_converter/universal-file-converter.c -o ufc $(LIBS) $(EXTRALIBS)

CLEAN += *.o ufc
